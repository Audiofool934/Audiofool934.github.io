---
import BaseLayout from "../layouts/BaseLayout.astro";
import AudioshowPost from "../components/AudioshowPost.astro";
const allPosts = await Astro.glob('./posts/audioshow/*.md')
const headTitle = "Audioshow";
const pageTitle = "Audioshow";
---

<BaseLayout pageTitle={pageTitle} headTitle={headTitle}>
    <ul>
        {allPosts.map((post) => <AudioshowPost url={post.url} episode={post.frontmatter.episode} pubDate={post.frontmatter.pubDate} title={post.frontmatter.title}/>)}
    </ul>
</BaseLayout>